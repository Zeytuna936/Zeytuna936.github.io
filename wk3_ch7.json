{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
      "text": "Renewables",
      "anchor": "start",
      "subtitle": "Output, GWh, | IEA via ECO-API",
      "fontSize": "18",
      "subtitleFontSize": "14",
      "color": "#000000",
      "subtitleColor": "#202424ce"
    },
    "height": "300",
    "width": "300",
    "autosize": {
      "type": "fit",
      "contains": "padding"
    },
    "background": "#ffffff",
    "view": {
      "fill": "#ffffff",
      "strokeOpacity": 0
    },
    "data": {
      "url": "https://api.economicsobservatory.com/gbr/rGWh?vega",
      "format": {
        "type": "json"
      }
    },
    "mark": {
      "type": "line",
      "color": "#05e005",
      "strokeWidth": "2"
    },
    "encoding": {
      "x": {
        "field": "date",
        "type": "temporal",
        "axis": {
          "title": "",
          "titleColor": "#122B39",
          "grid": false
        }
      },
      "y": {
        "field": "value",
        "type": "quantitative",
        "axis": {
          "title": "",
          "titleColor": "#122B39",
          "grid": false
        }
      }
    }
  }